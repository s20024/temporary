{
  "manifest_version": 2,
  "name": "TestMemo",
  "version": "1.0.0",
  "description": "ここにアプリケーションの概要を記入する。",
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": ["tabs","storage"],
  "browser_action": {
    "default_title": "ChromeMemo",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/contents.js"]
    }
  ]
}
